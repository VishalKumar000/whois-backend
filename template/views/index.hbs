<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="styles.css" />
    <title>Whois Domain Lookup</title>
  </head>
  <body>
    <div class="container">
      <div class="title">Whois Domain Lookup</div>
      <br />
      <div class="example">
        Example:
        <span>https://www.odinproject.com,&nbsp;&nbsp; https://github.com
        </span>
      </div>
      <form action="/" method="POST" class="input-container">
        <input
          type="text"
          name="domain"
          class="input"
          pattern="https?:\/\/(www\.)?[-a-zA-Z0-9@:%._\+~#=]{1,256}\.[a-zA-Z0-9()]{1,6}\b([-a-zA-Z0-9()@:%_\+.~#?&//=]*)"
          required
          placeholder="Enter your domain"
        />
        <button class="btn">SEARCH</button>
      </form>

      <h1>Some Recent Search Domain</h1>
      {{#each domainNames}}
        <li>{{this.title}}</li>
      {{/each}}
    </div>
  </body>
</html>